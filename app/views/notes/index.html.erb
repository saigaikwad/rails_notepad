<p style="color: green"><%= notice %></p>


<h3 class="container"> welcome <%= current_user.email%></h3>





<div class="container">
  <div class="row">
    <% @notes.each do |note| %>
    <% if note.user == current_user%>
      <div class="col-md-4 mb-4">
        <div class="card h-100">
          <div class="card-body">
            <h5 class="card-title"><%= render note %></h5>
            <a href="<%= note_path(note) %>" class="card-link">Show this note</a>
          </div>
        </div>
      </div><%end%>
    <% end %>
  </div>
</div>



